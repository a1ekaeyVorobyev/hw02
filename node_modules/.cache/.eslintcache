oAuth = localStorage.getItem(auth.login);\n  if (oAuth == null)\n    return false\n  const storageAuth =  JSON.parse(oAuth);\n  return storageAuth.password == auth.password\n};\n\nexport function setLogin(auth:tAuth){\n  const oAuth = localStorage.getItem(auth.login);\n  if (oAuth != null){\n    return false;\n  }\n  localStorage.setItem(auth.login, JSON.stringify(auth))\n  return true\n};","/home/a1eksey/project/OTUS_ASP/REACT/hw02/src/Hooks/useAuth.tsx",[],[],"/home/a1eksey/project/OTUS_ASP/REACT/hw02/src/HOC/withCard.tsx",["295","296"],[],"import React, { useContext } from \"react\";\nimport { MDBCard, MDBCardBody, MDBContainer } from \"mdb-react-ui-kit\";\nimport { withTheme } from \"./withTheme\";\nimport Main from \"../Components/Main\";\nimport About from \"../Components/About\";\nimport NotFound from \"../Components/NotFound\";\nimport Login from \"../Components/Login\";\nimport Registration from \"../Components/Registration\";\nimport ClearLocalStorage from \"../Components/ClearLocalStorage\";\nimport { iTheme } from \"../Interfaces/iTheme\";\n\ninterface AwaysCoolStateProps {\n  text?: string;\n}\n\nexport function withCard(ChildComp: React.ComponentType<any >) {\n    return class Component<T> extends React.Component< T&iTheme> \n    {\n     render() {\n        return (\n            <MDBContainer >\n              <MDBCard>\n                <MDBCardBody>\n                   <ChildComp {...this.props}/>\n                  </MDBCardBody>\n              </MDBCard>\n            </MDBContainer>\n        );\n      }\n    };\n  }\n\n\nexport const AppReg = withTheme(withCard(Registration));\nexport const AppLogin = withTheme(withCard(Login));\nexport const AppAbout = withTheme(withCard(About));\nexport const AppMain = withTheme(withCard(Main));\nexport const AppNotFound = withTheme(withCard(NotFound));\n\nexport const AppClearLocalStorage = withTheme(withCard(ClearLocalStorage));\n\n\n\n\n",["297","298","299","300","301","302"],"/home/a1eksey/project/OTUS_ASP/REACT/hw02/src/Components/NotFound.tsx",[],[],"/home/a1eksey/project/OTUS_ASP/REACT/hw02/src/HOC/withTheme.tsx",["303","304","305","306","307"],[],"/home/a1eksey/project/OTUS_ASP/REACT/hw02/src/Components/Registration.tsx",["308","309","310","311","312","313","314"],[],"import React, { useState } from \"react\";\nimport { Button, ButtonGroup, Col, Form, Row } from \"react-bootstrap\";\nimport { useNavigate } from \"react-router-dom\";\nimport {\n  getAuth,\n  setLogin,\n  tAuth,\n  useAuthContext,\n} from \"../Hooks/useAuthContext\";\nimport \"./Registration.css\";\n\nfunction Registration() {\n  const { auth, setAuth } = useAuthContext();\n  const [reg ,setReg] = useState(auth.login);\n  const navigate = useNavigate();\n\n  let login: string;\n  let password: string;\n  let confirmPassword: string;\n  //let error:string = '';\n  const [error, setError] = useState(\"\");\n  const onClickOK = (event: React.MouseEvent<HTMLButtonElement>) => {\n    if (password == confirmPassword) {\n      if (setLogin({ login: login, password: password }) == true) {\n        setReg(login);\n        navigate(\"/MyApp\");\n      }\n    } else {\n      setError(\"Нету пользователя или пароль введен с ошибкой\");\n    }\n    event.preventDefault();\n  };\n  return (\n    <div className=\"form\">\n      <div className=\"form-body\">\n        <Form.Group className=\"mb-3\" controlId=\"exampleForm.ControlInput1\">\n          <Form.Label>Login</Form.Label>\n          <Form.Control\n            size=\"lg\"\n            type=\"email\"\n            placeholder=\"name@example.com\"\n            onChange={(e) => {\n              login = e.target.value;\n            }}\n          />\n        </Form.Group>\n\n        <Form.Group className=\"mb-3\" controlId=\"exampleForm.ControlInput1\">\n          <Form.Label>Password</Form.Label>\n          <Form.Control\n            size=\"lg\"\n            type=\"password\"\n            onChange={(e) => {\n              password = e.target.value;\n            }}\n          />\n        </Form.Group>\n        <Form.Group className=\"mb-3\" controlId=\"exampleForm.ControlInput1\">\n          <Form.Label>Confirm Password</Form.Label>\n          <Form.Control\n            size=\"lg\"\n            type=\"password\"\n            onChange={(e) => {\n              confirmPassword = e.target.value;\n            }}\n          />\n        </Form.Group>\n      </div>\n      {/* <div className=\"btn-group d-flex\" role=\"group\"> */}\n      <div>\n        <h2 className=\"errorMessage\">{error}</h2>\n      </div>\n      <Row>\n        <Col className=\"btn-group d-flex\">\n          <Button variant=\"primary\" size=\"lg\" onClick={onClickOK}>\n            Ok\n          </Button>\n        </Col>\n      </Row>\n    </div>\n  );\n}\n\nexport default Registration;\n","/home/a1eksey/project/OTUS_ASP/REACT/hw02/src/Components/Main.tsx",[],[],"/home/a1eksey/project/OTUS_ASP/REACT/hw02/src/Components/About.tsx",[],[],"/home/a1eksey/project/OTUS_ASP/REACT/hw02/src/Components/ClearLocalStorage.tsx",[],[],"/home/a1eksey/project/OTUS_ASP/REACT/hw02/src/Components/Login.tsx",["315"],[],"import React, { useState } from \"react\";\nimport { Button, ButtonGroup, Col, Form, Row } from \"react-bootstrap\";\nimport { useNavigate } from \"react-router-dom\";\nimport {  getAuth, useAuthContext } from \"../Hooks/useAuthContext\";\nimport \"./Login.css\";\n\nfunction Login(login?:string) {\n   const auth = useAuthContext();\n   \n\n  const navigate = useNavigate();\n\n  const onClickCancel = (event: React.MouseEvent<HTMLButtonElement>) => {\n    navigate(\"/MyApp\");\n    event.preventDefault();\n  };\n  const onClickReg = (event: React.MouseEvent<HTMLButtonElement>) => {\n    navigate(\"/Registration\");\n    event.preventDefault();\n  };\n\n  let user: string = \"name@example.com\";\n  let password: string = '';\n  //let error:string = '';\n  const [error, setError] = useState(\"\");\n  const onClickOK = (event: React.MouseEvent<HTMLButtonElement>) => {\n    //  